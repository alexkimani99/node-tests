<%- include("template/cabecera", {tituloWeb: 'Crear nueva ciudad' }) %>
    <div class="container">
        <h1>Crear nueva ciudad</h1>
        <form action="/ciudad" class="my-2" method="POST">
            <input type="text" placeholder="Tipos de pokemon encontrados" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Region Ciudad" class="form-control my-2" name="tipo">
            <input type="text" placeholder="Gimnasio de la Ciudad" class="form-control my-2" name="descripcion">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
<%- include("template/footer") %>